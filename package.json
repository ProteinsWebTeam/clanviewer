{
  "name": "clanviewer",
  "description": "A component to visualise the relationships between the Pfam families in a clan",
  "version": "0.1.11",
  "homepage": "https://github.com/ProteinsWebTeam/clanviewer",
  "author": {
    "name": "4ndr01d3",
    "email": "gsalazar@ebi.ac.uk"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ProteinsWebTeam/clanviewer.git"
  },
  "bugs": {
    "url": "https://github.com/ProteinsWebTeam/clanviewer/issues"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://github.com/ProteinsWebTeam/clanviewer/blob/master/LICENSE"
    }
  ],
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "main": "build/clanviewer.packed.js",
  "scripts": {
    "start": "webpack",
    "dev": "webpack-dev-server",
    "lint": "eslint .",
    "jest": "jest",
    "test": "npm run lint && npm run jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testURL": "http://localhost"
  },
  "dependencies": {
    "d3-drag": "1.2.3",
    "d3-force": "2.0.0",
    "d3-scale": "2.1.2",
    "d3-selection": "1.3.2",
    "d3-zoom": "1.7.3",
    "resize-observer-polyfill": "1.5.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "coveralls": "3.0.2",
    "eslint": "5.10.0",
    "eslint-plugin-jest": "22.1.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "sniper": {
    "js": [
      "https://cdn.jsdelivr.net/npm/clanviewer@latest/build/clanviewer.packed.js"
    ],
    "css": [
      "/css/clanviewr.css"
    ],
    "snippets": [
      "biojs"
    ],
    "first": "example"
  },
  "keywords": [
    "pfam",
    "protein families",
    "clans",
    "d3",
    "biojs"
  ]
}

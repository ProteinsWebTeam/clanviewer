      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "d3-drag": "^1.2.1",
    "d3-force": "^1.1.0",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-zoom": "^1.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-plugin-jest": "^21.2.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "keywords": [
    "pfam",
    "protein families",
    "clans",
    "d3",
    "biojs"
  ]
